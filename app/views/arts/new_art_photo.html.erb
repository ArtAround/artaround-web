<div class="addArtContainer">
	<div id="titleContent">
		<h2>Add Art</h2>
	</div>
	<div id="ltColumn">
		<%= form_for(@photo, :url => create_art_photo_path) do |f| %>
				<%= f.label :image %>
				<%= f.file_field :image %><br />
				<%= f.label :attribution_text %>
				<%= f.text_field :attribution_text %><br />
				<%= f.label :attribution_url %>
				<%= f.text_field :attribution_url %><br />
			<%= f.submit %>
		<% end %>
	</div>
</div>